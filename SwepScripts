if game.PlaceId == 662417684 then
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Swep Scripts | ‚ùì LUCKY BLOCKS Battlegrounds", HidePremium = false, SaveConfig = true, ConfigFolder = "LuckyBattleground", IntroText = "Swep Scripts"})

OrionLib:MakeNotification({
	Name = "Welcome!",
	Content = "Thanks For Using Our Script",
	Image = "http://www.roblox.com/asset/?id=8285267398",
	Time = 5
})

--Misc tab
local Tab = Window:MakeTab({
	Name = "Misc",
	Icon = "http://www.roblox.com/asset/?id=9279161980",
	PremiumOnly = false
})

--Section
local MiscSection = Tab:AddSection({
	Name = "Character Setting"
})
Tab:AddSlider({
	Name = "WalkSpeed",
	Min = 20,
	Max = 300,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
		print(s)
	end    
})
Tab:AddSlider({
	Name = "jumpPower",
	Min = 50,
	Max = 300,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(j)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
		print(j)
	end    
})

--Infinity Yield
local Section = Tab:AddSection({
	Name = "Infinity Yield"
})
Tab:AddButton({
	Name = "Infinity Yeild",
	Callback = function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
      		print("button pressed")
  	end    
})

--emotions, animations
local Section = Tab:AddSection({
	Name = "Character Emotions, Animations"
})
Tab:AddButton({
	Name = "Emotions, Animations",
	Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/raw/LmyZr87L"))()
      		print("button pressed")
  	end    
})

--LuckyBlock tab
local Tab = Window:MakeTab({
	Name = "LuckyBlocks",
	Icon = "http://www.roblox.com/asset/?id=8277401585",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Open LuckyBlocks"
})
Tab:AddButton({
	Name = "Normal LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnLuckyBlock:FireServer()
        print("Opened")
  	end
})
Tab:AddButton({
	Name = "Super LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnSuperBlock:FireServer()
        print("Opened")
  	end
})
Tab:AddButton({
	Name = "Diamond LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnDiamondBlock:FireServer()
        print("Opened")
  	end
})
Tab:AddButton({
	Name = "Rainbow LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnRainbowBlock:FireServer()
        print("Opened")
  	end
})
Tab:AddButton({
	Name = "Galaxy LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnGalaxyBlock:FireServer()
        print("Opened")
  	end
})
Tab:AddButton({
	Name = "Limited Time LuckyBlock",
	Callback = function(Value)
        game.ReplicatedStorage.SpawnLuckyBlock:FireServer()
        game.ReplicatedStorage.SpawnSuperBlock:FireServer()
        game.ReplicatedStorage.SpawnSuperBlock:FireServer()
        game.ReplicatedStorage.SpawnDiamondBlock:FireServer()
        game.ReplicatedStorage.SpawnDiamondBlock:FireServer()
        game.ReplicatedStorage.SpawnRainbowBlock:FireServer()
        game.ReplicatedStorage.SpawnRainbowBlock:FireServer()
        game.ReplicatedStorage.SpawnGalaxyBlock:FireServer()
        game.ReplicatedStorage.SpawnGalaxyBlock:FireServer()
        print("Opened")
  	end
